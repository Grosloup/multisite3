{% extends "ZPBAdminBundle:Layouts:master.html.twig" %}

{% block title %}Les FAQs{% endblock %}
{% set active = "list_faq" %}

{% block content %}
    <div class="row">
        <div class="column-12">
            <div class="tile">
                <div class="tile-header">
                    Liste des questions fr√©quentes
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="column-12">
            <div class="tile">
                <table class="table table-hover table-striped">
                    <thead>
                    <tr>
                        <th>#Id</th>
                    </tr>
                    </thead>
                    <tbody>
                    {% for faq in faqs %}
                        <tr>
                            <td>{{ faq.id }}</td>
                        </tr>
                    {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
{% endblock %}
