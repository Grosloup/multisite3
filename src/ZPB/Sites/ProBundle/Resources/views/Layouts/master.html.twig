{% set active_page = (active_page is defined) ? active_page : "" %}
<!doctype html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>{% if block('title') %}{{ block('title')}}{% else %}Bienvenue{% endif %} | pro.zoobeauval.com</title>
    {% if block('description') %}
        <meta name="description" content="{{ block('description') }}"/>
    {% endif %}
    {% if block('keywords') %}
        <meta name="keywords" content="{{ block('keywords') }}"/>
    {% endif %}

    {% block headLinks%}
        <link rel="stylesheet" href="/css/font-awesome.min.css"/>
        <link rel="stylesheet" href="/css/sites/pro/master.css"/>
    {% endblock %}

    {% block headScripts %}
        <script src="/js/vendor/require.js" data-main="/js/sites/pro/main.js"></script>
    {% endblock %}

    {% block headStyles %}

    {% endblock %}
</head>
<body>
{% block topnav %}
    {{ render(controller("ZPBSitesCommonBundle:Navs:topnav", {url: path("zpb_sites_pro_english_index")})) }}
{% endblock %}

{% block main %}
    {% block header %}
        <div id="site-header">
            {% block mainbar %}
                {{ render(controller("ZPBSitesProBundle:Navs:mainbar", {active_page: active_page})) }}
            {% endblock %}
            {{ header_carousel("ZPBSitesCommonBundle:incs:header_carousel.html.twig") }}
        </div>
    {% endblock %}

    <div class="main-content">
        {% block content %}{% endblock %}
    </div>


{% endblock %}
{% block secondary_mainbar %}
    {{ render(controller("ZPBSitesProBundle:Navs:secondaryMainbar", {active_page: active_page})) }}
{% endblock %}
{% block footer %}
    {{ render(controller("ZPBSitesProBundle:Partials:footer")) }}
{% endblock %}



</body>
</html>
